# Comma-separated bearer API keys for clients
API_KEYS=replace_me_with_a_long_random_string

# Optional: 32-byte encryption key (hex/base64/passphrase)
SCHEDULER_ENCRYPTION_KEY=replace_me

HOST=0.0.0.0
PORT=0
DATA_DIR=.data
SCHEDULER_POLL_MS=5000
RATE_LIMIT_MAX=120
RATE_LIMIT_WINDOW_MS=60000

# Optional HTTPS (set both or neither)
# TLS_KEY_PATH=/absolute/path/to/key.pem
# TLS_CERT_PATH=/absolute/path/to/cert.pem
